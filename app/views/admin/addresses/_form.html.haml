= simple_form_for(@address) do |f|
  = f.error_notification

  .form-inputs
    = f.input :first_name
    = f.input :last_name
    = f.input :address
    = f.input :city
    = f.input :state
    = f.input :zip_code
    = f.input :phone_number
    = f.input :alternative_phone
    = f.input :default

  .form-actions
    = f.button :submit
